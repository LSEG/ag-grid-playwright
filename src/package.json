{
  "name": "ag-grid-playwright",
  "version": "1.1.2",
  "description": "Test ag-grid using Playwright in a simple, consistent, and extensive way. You can test simple rows and columns or extend it to test ag-grid from playwright in it's entirety including ag-grid enterprise.",
  "main": "lib/lib.cjs.js",
  "module": "lib/lib.js",
  "types": "lib/lib.d.ts",
  "type": "module",
  "scripts": {
    "clean": "find lib -mindepth 1 -delete",
    "build": "npm run clean && cp ./src/aggrid-test-util.js ./lib/aggrid-test-util.js && rollup -c && cp ../README.md ../src/README.md",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Joel Carras, Eoin Reilly",
  "keywords": [
    "playwright",
    "ag-grid",
    "testing",
    "test util",
    "frontend testing"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.34"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LSEG/ag-grid-playwright.git"
  },
  "license": "MIT"
}
